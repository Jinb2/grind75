class Solution:
    def longestConsecutive(self, nums: List[int]) -> int:

        maxLen = 0
        numSet = set(nums)

        for num in numSet:

            if num - 1 not in numSet:

                length = 0

                while num in numSet:
                    num += 1
                    length += 1

                maxLen = max(maxLen, length)

        return maxLen
